{
  "expo": {
    "name": "Uznews",
    "slug": "Uznews",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "facebookScheme": "fb1031789204286441",
    "facebookAppId": "1031789204286441",
    "facebookDisplayName": "Uznews-2",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
